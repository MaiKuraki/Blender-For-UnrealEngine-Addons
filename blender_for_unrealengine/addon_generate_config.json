{
    "schema_version": [1,0,0],
    "blender_manifest": {
        "version": [4,4,3],
        "id": "unrealengine_assets_exporter",
        "name": "Unreal Engine Assets Exporter",
        "tagline": "Allows to batch export and import in Unreal Engine",
        "maintainer": "Loux Xavier (BleuRaven) xavierloux.loux@gmail.com",

        "website_url": "https://github.com/xavier150/unrealengine_assets_exporter-Addons/",
        "report_issue_url": "https://github.com/xavier150/unrealengine_assets_exporter-Addons/issues",
        "documentation_url": "https://github.com/xavier150/unrealengine_assets_exporter-Addons/wiki",
        "support": "COMMUNITY",

        "type": "add-on",
        "tags": ["Import-Export"],
        "category": "Import-Export",
        "license": ["SPDX:GPL-3.0-or-later"],
        
        "copyright": [
        "2025 Xavier Loux",
        "2013 Blender Foundation",
        "2006-2012 assimp team",
        "2013 Campbell Barton",
        "2014 Bastien Montagne"
        ],
                
        "permissions": {
            "files": "Import/export FBX from/to disk",
            "clipboard": "Copy generated script paths"
        }
    },

    "builds": {
        "unrealengine_assets_exporter_4.5": {   
            "generate_method": "EXTENTION_COMMAND",
            "auto_install_range": [[4,5,0], "LATEST"],
            "naming": "{Name}_{Version}-blender_4.5.zip",
            "module": "unrealengine_assets_exporter", 
            "pkg_id": "unrealengine_assets_exporter",
            "exclude_paths": [
                "fbxio/",
                "addon_debug/",
                "languages/check_translation.py"
            ],
            "include_paths": [
                "fbxio/__init__.py/",
                "fbxio/io_scene_fbx_4_5/"
            ],
            "blender_version_min": [4,5,0]
        },
        "unrealengine_assets_exporter_4.4": {   
            "generate_method": "EXTENTION_COMMAND",
            "auto_install_range": [[4,4,0], [4,4,3]],
            "naming": "{Name}_{Version}-blender_4.4.zip",
            "module": "unrealengine_assets_exporter", 
            "pkg_id": "unrealengine_assets_exporter",
            "exclude_paths": [
                "fbxio/",
                "addon_debug/",
                "languages/check_translation.py"
            ],
            "include_paths": [
                "fbxio/__init__.py/",
                "fbxio/io_scene_fbx_4_4/"
            ],
            "blender_version_min": [4,4,0]
        },
        "unrealengine_assets_exporter_4.3": {   
            "generate_method": "EXTENTION_COMMAND",
            "auto_install_range": [[4,3,0], [4,3,2]],
            "naming": "{Name}_{Version}-blender_4.3.zip",
            "module": "unrealengine_assets_exporter", 
            "pkg_id": "unrealengine_assets_exporter",
            "exclude_paths": [
                "fbxio/",
                "addon_debug/",
                "languages/check_translation.py"
            ],
            "include_paths": [
                "fbxio/__init__.py/",
                "fbxio/io_scene_fbx_4_3/"
            ],
            "blender_version_min": [4,3,0]
        },
        "unrealengine_assets_exporter_4.2": {   
            "generate_method": "EXTENTION_COMMAND",
            "auto_install_range": [[4,2,0], [4,2,9]],
            "naming": "{Name}_{Version}-blender_4.2.zip",
            "module": "unrealengine_assets_exporter", 
            "pkg_id": "unrealengine_assets_exporter",
            "exclude_paths": [
                "fbxio/",
                "addon_debug/",
                "languages/check_translation.py"
            ],
            "include_paths": [
                "fbxio/__init__.py/",
                "fbxio/io_scene_fbx_4_2/"
            ],
            "blender_version_min": [4,2,0]
        },
        "unrealengine_assets_exporter_2.9": {
            "generate_method": "SIMPLE_ZIP",
            "auto_install_range": [[2,90,0], [4,1,1]],
            "naming": "{Name}_{Version}-blender_2.9-4.1.zip",
            "module": "unrealengine_assets_exporter", 
            "pkg_id": "unrealengine_assets_exporter",
            "exclude_paths": [
                "fbxio/",
                "addon_debug/",
                "languages/check_translation.py"
            ],
            "include_paths": [
                "fbxio/__init__.py/",
                "fbxio/io_scene_fbx_4_1/",
                "fbxio/io_scene_fbx_4_0/",
                "fbxio/io_scene_fbx_3_6/",
                "fbxio/io_scene_fbx_3_5/", 
                "fbxio/io_scene_fbx_3_4/", 
                "fbxio/io_scene_fbx_3_3/", 
                "fbxio/io_scene_fbx_3_2/", 
                "fbxio/io_scene_fbx_3_1/", 
                "fbxio/io_scene_fbx_2_93/"
            ],
            "blender_version_min": [2,90,0]
        },
        "unrealengine_assets_exporter_2.8": {
            "generate_method": "SIMPLE_ZIP",
            "auto_install_range": [[2,80,0], [2,83,20]],
            "naming": "{Name}_{Version}-blender_2.8.zip",
            "module": "unrealengine_assets_exporter", 
            "pkg_id": "unrealengine_assets_exporter",
            "exclude_paths": [
                "fbxio/",
                "addon_debug/",
                "languages/check_translation.py"
            ],
            "include_paths": [
                "fbxio/__init__.py/",
                "fbxio/io_scene_fbx_2_83/"
            ],
            "_comment": "Workaround: Remove 'override' argument for Blender 2.8 compatibility.",
            "hard_modifications": {
                "replace": [
                    {"search": "override={'LIBRARY_OVERRIDABLE'},", "replace": ""},
                    {"search": "override={'LIBRARY_OVERRIDABLE'}", "replace": ""},
                    {"search": "override={'LIBRARY_OVERRIDABLE', 'USE_INSERTION'},", "replace": ""},
                    {"search": "override={'LIBRARY_OVERRIDABLE', 'USE_INSERTION'}", "replace": ""}
                ]
            },
            "blender_version_min": [2,80,0]
        }
    }
}
